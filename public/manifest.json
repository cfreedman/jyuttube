{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Jyuttube",
  "action": {
    "default_title": "Jyuttube",
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "storage"],
  "host_permissions": [
    "http://youtube.com/*",
    "https://youtube.com/*",
    "http://*.youtube.com/watch?*",
    "https://*.youtube.com/watch?*"
  ]
}
